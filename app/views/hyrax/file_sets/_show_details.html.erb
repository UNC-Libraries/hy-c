<%# [hyc-override] Overriding to make depositor a facet search and not link to user profile and display date_created instead of date_uplaoded if present %>
<h2><%= t(".file_details") %></h2>
<dl class="dl-horizontal file-show-term file-show-details">
  <dt><%= t(".depositor") %></dt>
  <dd itemprop="accountablePerson" itemscope itemtype="http://schema.org/Person"><span itemprop="name"><%= link_to @presenter.depositor, main_app.search_catalog_path(f: { depositor_ssim: [@presenter.depositor]}) %></span></dd>
  <dt><%= t(".date_uploaded") %></dt>
  <dd itemprop="datePublished"><%= @presenter.date_created.first || @presenter.date_uploaded %></dd>
  <dt><%= t(".date_modified") %></dt>
  <dd itemprop="dateModified"><%= @presenter.date_modified %></dd>
  <dt><%= t(".fixity_check") %></dt>
  <dd><%= @presenter.fixity_check_status %></dd>
  <dt><%= t(".characterization") %></dt>
  <dd>
    <% if @presenter.characterized? %>
      <%= render 'show_characterization_details' %>
    <% else %>
      <%= t(".not_yet_characterized") %>
    <% end %>
  </dd>
</dl>