<%# [hyc-override] Overriding to hide tombstoned files %>
<% if (current_user && current_user.admin?) || (@presenter.workflow.badge.present? &&
    /Tombstoned|Pending.deletion/.match(@presenter.workflow.badge).nil? && Hyrax.config.display_media_download_link?) %>
  <div class="no-preview">
    <%= t('hyrax.works.show.no_preview') %>
      <p /><%= link_to t('hyrax.file_set.show.download'),
        hyrax.download_path(file_set),
        id: "file_download",
        data: { label: file_set.id },
        target: "_new" %>
  </div>
<% end %>
