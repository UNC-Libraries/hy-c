<%# [hyc-override] Overriding to change tombstoned label %>
<% presenter.title.each_with_index do |title, index| %>
  <div class="row">
    <div class="col-sm-8">
      <% if index == 0 %>
        <h2><%= title %>
          <% if /Tombstoned/.match(presenter.workflow.badge).nil? %>
            <small class="text-muted"><%= presenter.permission_badge %> <%= presenter.workflow.badge %></small>
            <% else %>
            <small class="text-muted"><%= presenter.permission_badge %> <%= content_tag(:span, "Withdrawn", class: "state state-tombstoned label label-primary") %></small>
          <%end %>
        </h2>
      <% else %>
        <h2><%= title %></h2>
      <% end %>
    </div>
    <div class="col-sm-4 text-right">
      <% if index == 0 %>
        <%= render "show_actions", presenter: presenter %>
      <% end %>
    </div>
  </div>
<% end %>