<% if f.object.multiple? key %>
  <%= f.input :date_issued, as: :multi_value, html5: true, placeholder: "Date Format YYYY-MM-DD",
              input_html: { class: 'date-input', value: f.object.date_issued || '' } %>
<% else %>
  <%= f.input :date_issued, as: :date, html5: true, placeholder: "Date Format YYYY-MM-DD",
              input_html: { class: 'date-input', value: f.object.date_issued || '' } %>
<% end %>
