<%# [hyc-override] Overriding partial in hyrax gem in order to make license single-valued %>
<%= f.input :license, as: :select,
            collection: LicenseService.select(params[:controller]),
            include_blank: true,
            item_helper: LicenseService.method(:include_current_value),
            selected: LicenseService.default_license(params[:controller]),
            input_html: { class: 'form-control' } %>