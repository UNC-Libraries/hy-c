<%# [hyc-override] Overriding partial in hyrax gem in order to pull from remote vocabulary %>
<%= f.input :subject,
            as: :multi_value,
            input_html: {
                class: 'form-control',
                data: { 'autocomplete-url' => "/authorities/search/assign_fast/all",
                        'autocomplete' => :subject }
            },
            required: :subject %>